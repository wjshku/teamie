{
  "name": "teamie-backend-functions",
  "version": "1.0.0",
  "description": "Teamie Backend - Cloud Functions for Firebase",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "deploy": "npm run build && firebase deploy --only functions",
    "deploy:all": "npm run build && firebase deploy",
    "logs": "firebase functions:log",
    "logs:follow": "firebase functions:log --follow",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "firebase",
    "cloud-functions",
    "firestore",
    "typescript",
    "teamie",
    "meeting-management"
  ],
  "author": "Teamie Team",
  "license": "MIT",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": "20"
  }
}